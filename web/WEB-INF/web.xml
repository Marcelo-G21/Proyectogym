<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarUsuarios</servlet-name>
        <servlet-class>Controller.ListarUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertEvaluacion</servlet-name>
        <servlet-class>Controller.InsertEvaluacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnviaRut</servlet-name>
        <servlet-class>Controller.EnviaRut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaEvaluaciones</servlet-name>
        <servlet-class>Controller.BusquedaEvaluaciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarUsuario</servlet-name>
        <servlet-class>Controller.EditarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarPorRut</servlet-name>
        <servlet-class>Controller.ListarPorRut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertarUsuario</servlet-name>
        <servlet-class>Controller.InsertarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarUsuario</servlet-name>
        <servlet-class>Controller.EliminarUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarUsuarios</servlet-name>
        <url-pattern>/ListarUsuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEvaluacion</servlet-name>
        <url-pattern>/InsertEvaluacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnviaRut</servlet-name>
        <url-pattern>/EnviaRut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaEvaluaciones</servlet-name>
        <url-pattern>/BusquedaEvaluaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarUsuario</servlet-name>
        <url-pattern>/EditarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarPorRut</servlet-name>
        <url-pattern>/ListarPorRut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertarUsuario</servlet-name>
        <url-pattern>/InsertarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarUsuario</servlet-name>
        <url-pattern>/EliminarUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
